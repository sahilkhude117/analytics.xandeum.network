
# Connection pooling URL (for Prisma queries)
# Create accelerate for database from https://www.prisma.io/accelerate
DATABASE_URL="postgres://postgres.xxx:[PASSWORD]@xxx.pooler.supabase.com:6543/postgres?pgbouncer=true"

# Get from: connect -> direct
DIRECT_URL="postgresql://postgres.xxx:[PASSWORD]@xxx.supabase.com:5432/postgres"

# Get from: Supabase Dashboard → Settings → Data API → Project URL
NEXT_PUBLIC_SUPABASE_URL="https://xxx.supabase.co"

# Get from: Supabase Dashboard → Settings → API → Project API keys → anon/public
NEXT_PUBLIC_SUPABASE_ANON_KEY="eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."

# Get from: Supabase Dashboard → Settings → API → Project API keys → service_role
SUPABASE_SERVICE_ROLE_KEY="eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."

IP_API_KEY="your api key"

# Redis REST URL
# Get from: Upstash Console → Database → REST API → UPSTASH_REDIS_REST_URL
UPSTASH_REDIS_REST_URL="https://xxx.upstash.io"

# Redis REST Token
# Get from: Upstash Console → Database → REST API → UPSTASH_REDIS_REST_TOKEN
UPSTASH_REDIS_REST_TOKEN="AXXXxxxx..."


# Bootstrap pNode for network discovery
# This is the primary node we'll call get-pods-with-stats on
PRPC_BOOTSTRAP_URL="http://173.212.207.32:6000/rpc"

# Fallback bootstrap nodes (comma-separated)
PRPC_FALLBACK_URLS="http://152.53.155.15:6000/rpc,http://77.53.105.8:6000/rpc"

# RPC timeout in milliseconds
PRPC_TIMEOUT_MS="10000"

# Max retries for failed RPC calls
PRPC_MAX_RETRIES="3"


# Node environment
NODE_ENV="development"

# Next.js app URL (for production)
NEXT_PUBLIC_APP_URL="http://localhost:3000"

